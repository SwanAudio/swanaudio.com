@require '_colors'

ul
  list-style: none
  padding: 0 0 0 10px
  margin: 0

  &>li
    margin-bottom: 15px

.tweet
  color: font-light-secondary
  padding-left: 19px
  position: relative

  &:before
    content: "\f099"
    display: inline-block
    color: twitter
    font-family: FontAwesome
    position: absolute
    top: 5px
    left: 0
