# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index
GET     /contact                    controllers.Application.contact
GET     /product                    controllers.Application.products

GET     /newsfeed                   controllers.Newsfeed.index

# Angular API's
GET     /product/list               controllers.Products.list
GET     /product/detail/:id         controllers.Products.detail(id: Long)

# STATIC ROUTES
# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
GET     /static/*file               controllers.WebJarAssets.at(file)
